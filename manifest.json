{
  "name": "Track Tab",
  "description": "Build an Extension!",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/assets/images/get_started16.png",
      "32": "/assets/images/get_started32.png",
      "48": "/assets/images/get_started48.png",
      "128": "/assets/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/assets/images/get_started16.png",
    "32": "/assets/images/get_started32.png",
    "48": "/assets/images/get_started48.png",
    "128": "/assets/images/get_started128.png"
  },
  "options_page": "sandbox.html",
  "commands": {
    "go-previous-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+Left",
        "mac": "Command+Shift+Left"
      },
      "description": "Go to the previous tab in the chain"
    },
    "go-next-tab": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "Command+Shift+Right"
      },
      "description": "Go to the next tab in the chain"
    },
    "rebuild-index": {
      "suggested_key": {
        "default": "Ctrl+Shift+Down",
        "mac": "Command+Shift+Down"
      },
      "description": "Rebuild the chain"
    }
  }
}
